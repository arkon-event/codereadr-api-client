{
	"name" : "arkon-event/codereadr-api-client",
	"license" : "MIT",
	"type" : "library",
	"description" : "API Client for codereadr",
	"homepage" : "https://github.com/arkon-event/codereadr-api-client",
	"autoload" : {
		"psr-4" : {
			"ArkonEvent\\CodeReadr\\" : "src"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"ArkonEvent\\CodeReadr\\Tests\\" : "tests"
		}
	},
	"authors" : [{
			"name" : "Ketil Stadskleiv",
			"email" : "ks@arkon.no"
		}
	],	
	"require" : {
		"php" : ">=7.1",
		"psr/log" : "^1",
		"php-http/client-implementation" : "^1.0",
		"php-http/httplug" : "^1.0",
		"php-http/discovery" : "^1.0",
		"php-http/message" : "^1.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^6",
		"php-http/guzzle6-adapter" : "^1.0"
	}
}